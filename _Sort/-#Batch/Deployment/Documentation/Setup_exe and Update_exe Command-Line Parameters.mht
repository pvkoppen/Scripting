' Get the user's path to <drive>\program files\installshield\<product =
name>
' by reading the App Paths key
Function GetMainPath()

	On Error Resume Next
=09
	Dim objRegistry
	Dim nReturn, nLength
	Dim strDir

	Set objRegistry =3D CreateObject ("InstallShield.Registry.1")
   =20
    ' Hack to get it to work with ISX, too
    If objRegistry Is Nothing Then
        Set objRegistry =3D CreateObject =
("ISExpInstallShield.Registry.1")
  	    nReturn =3D objRegistry.RegReadValue (2, =
"SOFTWARE\Microsoft\Windows\CurrentVersion\App Paths\ISIDE.EXE", "Path", =
1, strDir)
	else
    	nReturn =3D objRegistry.RegReadValue (2, =
"SOFTWARE\Microsoft\Windows\CurrentVersion\App Paths\ISDEV.EXE", "Path", =
1, strDir)
    End If=09

	If nReturn < 0 Then
		GetMainPath =3D "C:\Program Files\InstallShield\Developer"
	Else
        nLength =3D len(strDir)     =20
        ' find second path (paths separated with semicolon)
        nReturn =3D InStr (strDir, ";")
        ' get path
        strDir =3D right (strDir, nLength - nReturn)
        nReturn =3D Instrrev (strdir, "\")
        strDir =3D left (strDir, nReturn - 1)
		GetMainPath =3D strDir
	End If

	Set objRegistry =3D Nothing

End Function
