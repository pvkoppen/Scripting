Sub DisplayDemo(strDemoName, strFeatureName, bPromptUser)
   =20
    Dim objApp, objExecute, objRegistry
    Dim bInstalled
    Dim strPath, strDemoFile, HTMLPath
   =20
    ' See if we're in the IDE before getting CApp
    If GetRefToApp =3D True Then
        Set objApp =3D m_objApp
    Else
        Set objApp =3D CreateObject("ISApp.CApp")
    End If
   =20
    ' See if the user needs to be prompted to install demos if necessary
    bInstalled =3D True
    If bPromptUser Then
        bInstalled =3D objApp.InstallIfNeeded (strFeatureName, =
IHELP_DEMOS_PROMPTTEXT)
    End If

    ' Show the demo
    If bInstalled Then
        Set objExecute =3D CreateObject (PROGID_ISUTIL_FILE())
        HTMLPath =3D LanguageDir()
        strPath =3D GetMainPath()
        strDemoFile =3D HTMLPath & strDemoName & ".dbd"
        strPath =3D strPath & "\System"
        objExecute.ShowDemo strPath, " -c " & strDemoFile
    End If
   =20
    Set objExecute =3D Nothing
    Set objRegistry =3D Nothing
    Set objApp =3D Nothing
   =20
End Sub
