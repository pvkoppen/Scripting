	PART "Check to enforce setting on; uncheck to enforce setting off" CHECKBOX
		VALUENAME "Allow WebDrives"
		VALUEON NUMERIC 0 ;reverse
		VALUEOFF NUMERIC 1
	END PART
	END POLICY
POLICY "Show Paste Options buttons"
	PART "Check to enforce setting on; uncheck to enforce setting off" CHECKBOX
	KEYNAME Software\Policies\Microsoft\Office\11.0\Common\General
		VALUENAME PasteOptions
		VALUEON NUMERIC 1
		VALUEOFF NUMERIC 0
	END PART
	END POLICY
POLICY "Web Folders: Managing pairs of Web pages and folders"
KEYNAME Software\Policies\Microsoft\Windows\CurrentVersion\Explorer
	PART "Web Folders: Managing pairs of Web pages and folders" DROPDOWNLIST
		VALUENAME NoFileFolderConnection
	ITEMLIST
			NAME "Show and manage the pair as a single file"		VALUE NUMERIC 0
			NAME "Show both parts and manage them individually"		VALUE NUMERIC 2
			NAME "Show both parts but manage as a single file"		VALUE NUMERIC 1
	END ITEMLIST
		NOSORT
	END PART
	END POLICY
POLICY "Do not automatically dismiss the New File task pane"
	PART "Check to enforce setting on; uncheck to enforce setting off" CHECKBOX
	KEYNAME Software\Policies\Microsoft\Office\11.0\Common\General
		VALUENAME DoNotDismissFileNewTaskPane
		VALUEON NUMERIC 1
		VALUEOFF NUMERIC 0
	END PART
	PART " " TEXT
	END PART
	PART "This setting prevents the New File task pane from being dismissed" TEXT
	END PART
	PART "automatically after creating a new file or opening an existing file." TEXT
	END PART
	END POLICY
POLICY "Disable web view in the Office file dialogs"
	PART "Check to enforce setting on; uncheck to enforce setting off" CHECKBOX
	KEYNAME Software\Policies\Microsoft\Office\11.0\Common\FileDialogWebViewSettings
		VALUENAME ShowAllFilesAndFolders
		VALUEON NUMERIC 1
		VALUEOFF NUMERIC 0
	END PART
	END POLICY
POLICY "Block updates from the Office Update Site from applying"
	PART "Check to disable updates from the Office Update site" CHECKBOX
	KEYNAME Software\Policies\Microsoft\Office\Common\OfficeUpdate
		VALUENAME BlockUpdates
		VALUEON NUMERIC 1
		VALUEOFF NUMERIC 0
	END PART
	PART "Setting this policy also disables the Check for Updates menu and " TEXT
	END PART
	PART "task pane items." TEXT
	END PART
	END POLICY
END CATEGORY
END CATEGORY


