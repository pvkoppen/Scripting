			VALUENAME TimeFocus			
			SPIN 1
			DEFAULT 60000
			MIN 0
			MAX 250000
	END PART
	END POLICY
POLICY "Specify duration of fade out (in millisec)"
KEYNAME Software\Policies\Microsoft\Office\11.0\Common\DesktopAlerts
	PART "Millisec:" NUMERIC
			VALUENAME TimeOut			
			SPIN 1
			DEFAULT 2000
			MIN 0
			MAX 25000
	END PART
	END POLICY
POLICY "Specify opacity of Desktop Alert"
KEYNAME Software\Policies\Microsoft\Office\11.0\Common\DesktopAlerts
	PART "Opacity (Alpha Level):" NUMERIC
			VALUENAME Opacity			
			SPIN 1
			DEFAULT 51
			MIN 0
			MAX 255
	END PART
	END POLICY
POLICY "Specify opacity at start of fade in"
KEYNAME Software\Policies\Microsoft\Office\11.0\Common\DesktopAlerts
	PART "Specify opacity at start of fade in:" NUMERIC
			VALUENAME FirstOpacity			
			SPIN 1
			DEFAULT 0
			MIN 0
			MAX 255
	END PART
	END POLICY
POLICY "Specify default location of Desktop Alert"
KEYNAME Software\Policies\Microsoft\Office\11.0\Common\DesktopAlerts
	PART "Corner (0-3)" NUMERIC
				VALUENAME Corner
				SPIN 1
				DEFAULT 3
				MIN 0
				MAX 3
	END PART
	PART "XOffset (default 44):" NUMERIC
				VALUENAME XOffset
	END PART
	PART "YOffset (default 42):" NUMERIC
				VALUENAME YOffset
	END PART
	PART "Corner corresponds to what quadrant of the screen the " TEXT
	END PART
	PART "notification should display in." TEXT
	END PART
	PART "0  |  1" TEXT
	END PART
	PART "---------" TEXT
	END PART
	PART "2  |  3" TEXT
	END PART
	PART "XOffset represents the horizontal distance from the selected corner." TEXT
	END PART
	PART "YOffset represents the vertical distance from the selected corner." TEXT
	END PART
	END POLICY
