CATEGORY "Tools | Spelling"
KEYNAME Software\Policies\Microsoft\Office\11.0\Publisher\Spelling
POLICY "Do not flag spelling errors in words containing mixed digits"
	PART "Check to enforce setting on; uncheck to enforce setting off" CHECKBOX
		VALUENAME IgnoreMixedDigits
		VALUEON 1
		VALUEOFF 0
	END PART
	END POLICY
POLICY "Do not flag spelling errors in words that look like URLs or e-mail addresses"
	PART "Check to enforce setting on; uncheck to enforce setting off" CHECKBOX
		VALUENAME IgnoreURLs
		VALUEON 1
		VALUEOFF 0
	END PART
	END POLICY
POLICY "Use the user dictionary for spelling correction suggestions"
	PART "Check to enforce setting on; uncheck to enforce setting off" CHECKBOX
		VALUENAME SuggestFromUserDict
		VALUEON 1
		VALUEOFF 0
	END PART
	END POLICY
POLICY "Arabic spelling options"
	PART "Arabic spelling options" DROPDOWNLIST
		VALUENAME ArabicMode
	ITEMLIST
			NAME "None"		VALUE 0
			NAME "Strict initial alef hamza"	VALUE 1
			NAME "Strict final yaa"		VALUE 2
			NAME "Both strict"		VALUE 3 DEFAULT
	END ITEMLIST
		NOSORT
	END PART
	END POLICY
POLICY "Hebrew spelling options"
	PART "Hebrew spelling options" DROPDOWNLIST
		VALUENAME HebrewMode
	ITEMLIST
			NAME "Full Script"		VALUE 0 DEFAULT
			NAME "Partial Script"		VALUE 1
			NAME "Mixed Script"		VALUE 2
			NAME "Mixed Authorized Script"	VALUE 3
	END ITEMLIST
		NOSORT
	END PART
	END POLICY
END CATEGORY
CATEGORY "Tools | Macro"
KEYNAME Software\Policies\Microsoft\Office\11.0\Publisher\Security
CATEGORY "Security..."
KEYNAME Software\Policies\Microsoft\Office\11.0\Publisher\Security
POLICY "Security Level"
	PART "Security Level" DROPDOWNLIST
		VALUENAME Level
	ITEMLIST
			NAME "Very High"	VALUE NUMERIC 4
			NAME "High"		VALUE NUMERIC 3	DEFAULT
			NAME "Medium"	VALUE NUMERIC 2
			NAME "Low"		VALUE NUMERIC 1
	END ITEMLIST
		NOSORT
	END PART
	END POLICY
POLICY "Trust all installed add-ins and templates"
	PART "Check to enforce setting on; uncheck to enforce setting off" CHECKBOX
		VALUENAME DontTrustInstalledFiles
		VALUEON NUMERIC 1
		VALUEOFF NUMERIC 0
	END PART
	END POLICY
END CATEGORY
