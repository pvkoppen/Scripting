<script language='JavaScript'>
<!--
function gss_GssSrchverify(){
	var errstr='';
	if(document.layers) document.GssSrchform.Query.value = escape(document.GssSrchform.Queryc.value);
	if(errstr != ''){return false;}
	else return true;
}

function gss_readGssSrchcookie(){
	if(document.domain.indexOf('.com') != -1) {
		document.domain = 'support.microsoft.com';
	}
	var srcform = document.GssSrchform;
	if(document.cookie != ''){
		var cs = document.cookie;
		var cookiename = 'GssSrch=';
		if(cs.indexOf(cookiename) > -1){
			startpoint = cs.indexOf(cookiename)+cookiename.length;
			endpoint = cs.indexOf(';', startpoint);
			if(endpoint < 0) endpoint = cs.length;
			cs = cs.substr(startpoint, endpoint-startpoint);
			var entryarray = new Array();
			var entryvalarray = new Array();
			entryarray = cs.split('&');
			for(i=0;i<entryarray.length;i++){
				entryvalarray = entryarray[i].split('=');
				if(entryvalarray[0] == 'Product' && srcform.Product.type != 'hidden'){
					for(j=0;j<srcform.Product.options.length;j++){
						if(srcform.Product.options[j].value == unescape(entryvalarray[1])){srcform.Product.selectedIndex = j; break;}
					}
				}
				if(entryvalarray[0] == 'Query' && '' == '' && entryvalarray[1] != '') srcform.Queryc.value = unescape(entryvalarray[1]);
				if(entryvalarray[0] == 'KeywordType' && srcform.KeywordType.type != 'hidden'){
					for(j=0;j<srcform.KeywordType.options.length;j++){
						if(srcform.KeywordType.options[j].value == unescape(entryvalarray[1])){srcform.KeywordType.selectedIndex = j; break;}
					}
				}else if(srcform.KeywordType.type == 'hidden'){srcform.KeywordType.value = unescape(entryvalarray[1]);}
				if(entryvalarray[0] == 'Titles' && srcform.Titles.type != 'hidden'){
					for(j=0;j<srcform.Titles.length;j++){
						if(srcform.Titles[j].value == unescape(entryvalarray[1])){srcform.Titles[j].checked = true; break;}
					}
				}else if(srcform.Titles.type == 'hidden') srcform.Titles.value = unescape(entryvalarray[1]);
				if(entryvalarray[0] == 'numDays' && srcform.numDays.type != 'hidden'){
					for(j=0;j<srcform.numDays.options.length;j++){
						if(srcform.numDays.options[j].value == unescape(entryvalarray[1])){srcform.numDays.selectedIndex = j; break;}
					}
				}else if(srcform.numDays.type == 'hidden'){srcform.numDays.value = unescape(entryvalarray[1]);}
				if(entryvalarray[0] == 'maxResults' && srcform.maxResults.type != 'hidden'){
					for(j=0;j<srcform.maxResults.options.length;j++){
						if(srcform.maxResults.options[j].value == unescape(entryvalarray[1])){srcform.maxResults.selectedIndex = j; break;}
					}
				}else if(srcform.maxResults.type == 'hidden'){srcform.maxResults.value = unescape(entryvalarray[1]);}
			}
		}
	}
}

function gss_insertval(srcstr, findstr, insertstr){
	if(srcstr.indexOf(findstr) > -1){
		sp = srcstr.indexOf(findstr)+findstr.length;
		ep = srcstr.indexOf('&', sp);
		if((srcstr.indexOf(';', sp) > 0 && srcstr.indexOf(';', sp) < ep) || ep < 0) ep = srcstr.indexOf(';', sp);
		if(ep < 0) ep = srcstr.length;
		return srcstr.substring(0, sp) + insertstr + srcstr.substring(ep, srcstr.length);
	}else return srcstr+'&'+findstr+insertstr;
}

function gss_writeGssSrchcookie(){
	if(!document.layers){
	setcookieval('srchDivPersist', '');
	if(document.domain.indexOf('.com') != -1) {
		document.domain = 'support.microsoft.com';
	}
	var uc = false;
	var cstr = 'GssSrch=';
	if(document.cookie.indexOf('GssSrch=') > -1) uc = true;
	if(uc){
		sp = document.cookie.indexOf('GssSrch=');
		ep = document.cookie.indexOf(';',sp);
		if(ep < 0) ep = document.cookie.length;
		cstr = document.cookie.substr(sp, ep-sp);
	}
	srcform = document.GssSrchform;

	if(srcform.Product.type != 'hidden'){
		if(uc) cstr = gss_insertval(cstr,'Product=',escape(srcform.Product.options[srcform.Product.selectedIndex].value));
		else cstr += 'Product='+escape(srcform.Product.options[srcform.Product.selectedIndex].value)+'&';
	}	if(document.GssSrchform.Queryc.value == 'Type your search words here'){document.GssSrchform.Queryc.value = '';}
		if(uc) cstr = gss_insertval(cstr,'Query=',escape(srcform.Queryc.value));
		else cstr += 'Query='+escape(srcform.Queryc.value)+'&';
		setcookieval('refinequery', document.GssSrchform.Queryc.value);
		srcform.Query.value = UnicodeFixup(escape(srcform.Queryc.value));
		//srcform.Queryc.value = '';
	if(srcform.KeywordType.type != 'hidden'){
		if(uc) cstr = gss_insertval(cstr,'KeywordType=',escape(srcform.KeywordType.options[srcform.KeywordType.selectedIndex].value));
		else cstr += 'KeywordType='+escape(srcform.KeywordType.options[srcform.KeywordType.selectedIndex].value)+'&';
	}else{
		if(uc){cstr = gss_insertval(cstr,'KeywordType=',escape(srcform.KeywordType.value));}
		else{cstr += 'KeywordType='+escape(srcform.KeywordType.value)+'&';}
	}
	if(srcform.Titles.type != 'hidden'){
		for(i=0;i<srcform.Titles.length;i++){
			if(srcform.Titles[i].checked){
				if(uc) cstr = gss_insertval(cstr,'Titles=',escape(srcform.Titles[i].value));
				else cstr += 'Titles='+escape(srcform.Titles[i].value)+'&';
			}
		}
	}else{
		if(uc) cstr = gss_insertval(cstr,'Titles=',escape(srcform.Titles.value));
		else cstr += 'Titles='+escape(srcform.Titles.value)+'&';
	}
	if(srcform.numDays.type != 'hidden'){
		if(uc) cstr = gss_insertval(cstr,'numDays=',escape(srcform.numDays.options[srcform.numDays.selectedIndex].value));
		else cstr += 'numDays='+escape(srcform.numDays.options[srcform.numDays.selectedIndex].value)+'&';
	}else{
		if(uc){cstr = gss_insertval(cstr,'numDays=',escape(srcform.numDays.value));}
		else{cstr += 'numDays='+escape(srcform.numDays.value)+'&';}
	}
	if(srcform.maxResults.type != 'hidden'){
		if(uc) cstr = gss_insertval(cstr,'maxResults=',escape(srcform.maxResults.options[srcform.maxResults.selectedIndex].value));
		else cstr += 'maxResults='+escape(srcform.maxResults.options[srcform.maxResults.selectedIndex].value)+'&';
	}else{
		if(uc){cstr = gss_insertval(cstr,'maxResults=',escape(srcform.maxResults.value));}
		else{cstr += 'maxResults='+escape(srcform.maxResults.value)+'&';}
	}

	if(cstr.charAt(cstr.length-1) == '&'){cstr = cstr.substring(0, cstr.length-1);}
	document.cookie = cstr+'; expires=Fri, 31 Dec 2002 23:59:59 GMT; domain=support.microsoft.com; path=/';
}
}

// -->
</script>
